    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.innosoft.testit",
      "supportsTablet": true,
      "usesIcloudStorage": true
    },
    "android": {
      "package": "com.innosoft.testit"
    }
  }
}
